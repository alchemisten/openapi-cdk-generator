{
  "name": "openapi-cdk-transformer",
  "version": "0.1.0",
  "description": "Library that transforms an OpenAPI specification to Typescript code",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "preversion": "npm run lint",
    "test": "jest --passWithNoTests"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alchemisten/openapi-cdk-generator.git"
  },
  "keywords": [
    "cdk",
    "typescript",
    "generator",
    "openapi",
    "open-api",
    "api-gateway",
    "lambda"
  ],
  "author": "Alchemisten AG",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/alchemisten/openapi-cdk-generator/issues"
  },
  "homepage": "https://github.com/alchemisten/openapi-cdk-generator#readme",
  "dependencies": {
    "typescript": "^4.0.5",
    "yaml": "^1.10.0",
    "openapi-types": "^7.0.1",
    "change-case": "^4.1.2"
  },
  "devDependencies": {
    "@types/node": "^14.14.14",
    "@types/jest": "^26.0.19",
    "@types/yaml": "^1.9.7",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "ts-jest": "^26.4.4",
    "ts-node": "^9.1.1",
    "ts-lint": "^4.5.1"
  }
}
